<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">

  
  <servlet>
  <servlet-name>userlogin</servlet-name>
  <servlet-class>com.training.AddUser</servlet-class>
  </servlet>
  
<servlet-mapping>
	<servlet-name>userlogin</servlet-name>
	<url-pattern>/addUser</url-pattern>
</servlet-mapping> 


  <servlet>
  <servlet-name>listCourses</servlet-name>
  <servlet-class>com.training.coursesHome</servlet-class>
  </servlet>
  
<servlet-mapping>
	<servlet-name>listCourses</servlet-name>
	<url-pattern>/Courses</url-pattern>
</servlet-mapping>

  <servlet>
  <servlet-name>addCourses</servlet-name>
  <servlet-class>com.training.addCourseDb</servlet-class>
  </servlet>
  
<servlet-mapping>
	<servlet-name>addCourses</servlet-name>
	<url-pattern>/addCourses</url-pattern>
</servlet-mapping> 

  <servlet>
  <servlet-name>addFeedback</servlet-name>
  <servlet-class>com.training.addFeedback</servlet-class>
  </servlet>
  
<servlet-mapping>
	<servlet-name>addFeedback</servlet-name>
	<url-pattern>/addFeedback</url-pattern>
</servlet-mapping>    
</web-app>